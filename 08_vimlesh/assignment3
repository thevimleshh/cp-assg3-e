#include<stdio.h>
int main(){
    int i,num,multi;
    printf("\nEnter a any table : ");
    scanf("%d",&num);
    printf("\n");
    for(i=1;i<=10;i++){
        multi=num*i;
        printf("%d * %d = %d\n",num,i,multi);
    }
    return 0;
}